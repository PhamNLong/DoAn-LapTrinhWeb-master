    <script type="text/javascript">
        $(function () {
            $("#slider-range").slider({
                range: true,
                min: 0,
                max: 1000000,
                values: [0, 1000000],
                slide: function (event, ui) {
                    $("#amount").val("$" + ui.values[0] + " - $" + ui.values[1]);
                }
            });
            $("#amount").val($("#slider-range").slider("values", 0) + "đ" +
                " - " + $("#slider-range").slider("values", 1) + "đ");
        });
    </script>


